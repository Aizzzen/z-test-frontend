{
    "presets": [
        [
            "@babel/preset-env",
            {
                "modules": false,
                "targets": {
                    "browsers": [
                        "> 1%",
                        "last 4 versions",
                        "ie > 9",
                        "Opera > 30",
                        "Firefox > 40.0",
                        "Safari > 5.1.7",
                        "Chrome > 43.0"
                    ]
                }
            }
        ]
    ],
    "plugins": ["transform-vue-jsx", "@babel/plugin-transform-runtime"]
}